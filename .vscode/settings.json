{
    "editor.semanticHighlighting.enabled": true,
    "Lua.diagnostics.globals": [
        "game",
        "script",
        "remote",
        "commands",
        "settings",
        "rcon",
        "rendering",
        "global",
        "defines",
        "serpent",
